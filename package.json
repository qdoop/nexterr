{
    "name": "nexterr",
    "displayName": "nextError",
    "description": "Automate and speed up your daily Build and GotoErrorLine development cycle",
    "version": "0.16.50923",
    "publisher": "qdoop",
    "icon": "images/qdoop.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/qdoop/nexterr.git"
    },
    "activationEvents": [
        "onCommand:extension.nextError",
        "workspaceContains:_next.json"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.nextError",
            "title": "nextError"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}